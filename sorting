#include<stdio.h>
void main()
{
    int i,j,temp,a[5];
    printf("enter a 5 values ");
    for(i=0;i<5;i++)
{
    scanf("%d",&a[i]);
}
for(i=0;i<5;i++)
{
    for(j=0;j<5;j++)
    {
        if(a[i]<a[j])
        {
            temp=a[i];
            a[i]=a[j];
            a[j]=temp;
        }
}
}
printf("the sorted values are\n");
for(i=0;i<5;i++)
printf("%d\n",a[i]);
}
